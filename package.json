{
  "name": "ts-starter",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "tsc": "tsc",
    "clean": "rm -rf dist",
    "build:dev": "tsc --build tsconfig.json -w",
    "build:prod": "rm -rf dist && tsc --build tsconfig.json",
    "build:web": "rm -rf public && mkdir public && cp src/index.html public/ && rollup -c",
    "start": "clear && node dist/main.js"
  },
  "dependencies": {
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.1.1",
    "rollup": "^2.3.2",
    "typescript": "^3.8.3"
  }
}
